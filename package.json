{
	"name": "vite-project",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"reload": "npm run close-set && npm run install-script && npm run open-set",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"install-script": "python3 runners/install.py --name LiveRemote",
		"watch-live-logs": "python3 runners/watch.py --user 'isaacpoole' --version 'Live 11.3.21'",
		"close-set": "pkill -x Ableton Live 11 Suite",
		"open-set": "open set/set.als",
		"generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/vite.svg"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-grid-layout": "^1.4.4",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.22.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vite-pwa/assets-generator": "^0.2.3",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"eslint": "^8.56.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"sass": "^1.70.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-pwa": "^0.17.5"
	}
}
