{
	"$schema": "https://biomejs.dev/schemas/1.2.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"style": {
				"useNamingConvention": "off"
			},
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"nursery": {
				"useImportRestrictions": "off"
			},
			"correctness": {
				"all": true
			}
		}
	},
	"formatter": {
		"enabled": true
	}
}
